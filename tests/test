def get_restaurant_addresses():
    restaurants = [
        {
            "address": "1299 Dough Ave N"
        },
        {
            "address": "6501 Island St SE"
        },
        {
            "address": "12543 Sushi Way NE"
        }
    ]

    addresses = []
    
    # no_addresses = restaurants[0]["address"]
    
    for restaurant in restaurants:
        addresses.append(restaurants[restaurant])

    print (addresses)
    #         no_addresses += no_addresses.append(address)
    
    return addresses
    
get_restaurant_addresses()
#return a list of restuarant addresses 
